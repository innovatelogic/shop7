		<server get="token" send="res"/>
	</msg>
	<msg name="logout">
		<client send="token" get="res"/>
		<server get="token" send="res"/>
	</msg>
	<msg name="get_groups">
		<client send="token;id" get="res"/>
		<server get="token;id" send="res"/>
	</msg>
	<msg name="get_category_childs">
		<client send="token;id" get="res"/>
		<server get="token;id" send="res"/>
	</msg>
	<msg name="get_items">
		<client send="token;category_id;offset" get="res"/>
		<server get="token;category_id;offset" send="res"/>
	</msg>
</messages>